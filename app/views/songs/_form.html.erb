<%= form_for @song do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :artist_id %>

  <%= f.collection_select :artist_id, Artist.all, :id, :name %>

  <% if false %>
  <%= f.collection_select :ends_up_in_params_as_key, Model.all, :ends_up_in_params_as_value, :display_method %>
  <% end %>

  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>

  <%= f.submit %>
<% end %>
